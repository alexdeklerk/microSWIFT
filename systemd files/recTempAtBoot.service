[Install]
WantedBy=multi-user.target

[Unit]
Documentation=man:systemd-sysv-generator(8)
SourcePath=/etc/init.d/recTempAtBoot
Description=LSB: Start recTemp at boot
After=network.target

[Service]
Type=forking
Restart=always
RestartSec=5
TimeoutSec=infinity
WatchdogSec=3600
KillMode=process
GuessMainPID=no
RemainAfterExit=no
SuccessExitStatus=5 6
ExecStart=/etc/init.d/recTempAtBoot start
ExecStop=/etc/init.d/recTempAtBoot stop
