[Install]
WantedBy=multi-user.target

[Unit]
Documentation=man:systemd-sysv-generator(8)
SourcePath=/etc/init.d/cpuTempWifi
Description=LSB: Start cpu temp and wifi at boot
After=network.target

[Service]
Type=forking
Restart=always
RestartSec=5
TimeoutSec=infinity
WatchdogSec=3600
KillMode=process
GuessMainPID=no
RemainAfterExit=no
SuccessExitStatus=5 6
ExecStart=/etc/init.d/cpuTempWifi start
ExecStop=/etc/init.d/cpuTempWifi stop
