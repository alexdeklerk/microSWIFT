[Install]
WantedBy=multi-user.target

[Unit]
Documentation=man:systemd-sysv-generator(8)
SourcePath=/etc/init.d/recordGPS
Description=LSB: Start recordGPS service at boot
After=network.target

[Service]
Type=forking	
Restart=always
RestartSec=5
TimeoutSec=infinity
KillMode=process
GuessMainPID=no
RemainAfterExit=no
SuccessExitStatus=5 6
ExecStart=/etc/init.d/recordGPS start
ExecStop=/etc/init.d/recordGPS stop

